/* pages/list/list.wxss */
.listItem{
  width: 100%;
  background: #F6F6F6;
  margin-bottom: 12rpx;
  padding-top: 16rpx;
  padding-left: 16rpx;
  padding-bottom: 16rpx;
}

.itemName{
  width: 100%;
  font-size: 16pt;
}

.itemDesc{
  width: 100%;
  font-size: 12pt;
  color: #353535;
}

/*加载更多与加载完毕*/
.loading{  
  padding: 10rpx;  
  text-align: center;  
}  
.loading:before{  
  /*display: inline-block;
  vertical-align: middle;  */  
  content: '';  
  width: 40rpx;  
  height: 40rpx;  
}  
.loading.complete:before{  
  display: none;  
}
.nodataset{
  display: flex;
  color: #888888;
  justify-content: center;
  margin-top: 500rpx;
}